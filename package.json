    "@babel/eslint-parser": "^7.24.5",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.26.0",
    "vue-template-compiler": "^2.7.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
